- meta title: t('.title')
- content_for :head do
  - javascript_include_tag 'highcharts', data: { turbolinks_track: 'reload' }

.hero.hero-color
  .hero__container
    %h1
      = t('.title')

%section.section.section-grey
  .container.container-full
    = render 'stats_params', stats: @stats
    = render 'stats/stats_charts', stats: @stats
