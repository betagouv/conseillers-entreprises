- meta title: t('.diagnosis')

%h1= t('.diagnosis')
- categories_with_questions.each do |item|
  %h2= item[:category]
  %table.ui.selectable.celled.table
    %tbody
      - item[:questions].each do |question|
        %tr
          %td= link_to question.label, question_visit_diagnosis_index_path(question.id, visit_id: params[:visit_id])