= paginator.render do
  %nav.fr-pagination.fr-mt-2w{ "aria-label" => t('.navigation_page'), role: 'navigation' }
    %ul.fr-pagination__list
      = first_page_tag
      = prev_page_tag
      - each_page do |page|
        - if page.left_outer? || page.right_outer? || page.inside_window?
          = page_tag page
        - elsif !page.was_truncated?
          = gap_tag
      = next_page_tag
      = last_page_tag
