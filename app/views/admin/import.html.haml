= form_for @active_admin_import_model, url: { action: :do_import }, html: { multipart: true } do |f|
  %fieldset.inputs
    %legend
      %span= t('active_admin.user.import.title')
    %ol
      %li
        = f.label :file, t('active_admin.user.import.label')
        = f.file_field :file, required: true
  %fieldset.actions
    %ol
      %li
        = f.submit t('active_admin.user.import.button'), data: { :disable_with => t("active_admin.user.import.button_disabled") }

- users = current_user.antenne.advisors.not_deleted.order(created_at: :desc)

%form
  %fieldset.inputs
    %legend
      %span= t('active_admin.user.import.example_title')
    %ol
      %li
        %label= t('active_admin.user.import.example_label')
        %textarea.textarea-monospace{ readonly: true, rows: users.size + 1 }
          = Admin::UserImporter::csv_example(users)
