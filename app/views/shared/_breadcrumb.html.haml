%nav.rf-breadcrumb{ 'aria-label': t('.you_are_here'), role: 'navigation' }
  %button.rf-breadcrumb__button{ 'aria-controls': 'breadcrumb-189', 'aria-expanded': 'false' }
    = t('.see_breadcrumb')
  .rf-collapse#breadcrumb-189
    %ol.rf-breadcrumb__list
      - pages.each do |page|
        %li
          = link_to page[:title], page[:path], class: 'rf-breadcrumb__link'
      %li
        %a.rf-breadcrumb__link{ 'aria-current': 'page' }
          = current_page
