- meta title: t('.meta_title'), description: t('.meta_description')
.fr-container.fr-py-7w.fr-mt-3v
  %h1.fr-h2= t('.title')
  %p.fr-text--lg= t('.subtitle')

= render 'pages/breadcrumbs', title: t('.title')

.fr-container--fluid.fr-py-7w#liste-partenaires
  .fr-container.sitemap
    .fr-grid-row.fr-grid-row--center
      .fr-col-12.fr-col-md-10.fr-col-lg-8
        - @site_map.each do |elements|
          = raw site_map_html(elements)
