%section.section.section-grey.landing-subject-section
  .container
    - landing_themes.each do |theme|
      %h2.card-section-header= theme.title || t('you_wish')
      .landing-subject-cards-container
        = render partial: 'landings/landing_subjects/card', collection: theme.landing_subjects, as: :landing_subject, locals: { landing: landing }

    - unless in_iframe?
      .ui.text-center.other
        %b= t('landings.landing_themes.show.back_to_other_subjects_html').html_safe
